<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="hspark" compute_IC="true"
    geometry_file="geometry/axi20x20xhalfr2.geom" output_format="ExodusII"
    title="Dielectric Elastomer Testing" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="100" output_inc="100" time_step="0.01">
        <schedule_function>
            <linear_function a="1" b="0"/>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement" integrator="nonlinear_HHT">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
                <String value="D_Z"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1" type="fixed"/>
            <kinematic_BC dof="1" node_ID="2" type="fixed"/>
            <kinematic_BC dof="2" node_ID="2" type="fixed"/>
            <kinematic_BC dof="1" node_ID="3" type="fixed"/>
            <kinematic_BC dof="2" node_ID="3" type="fixed"/>
            <kinematic_BC dof="2" node_ID="4" type="fixed"/>
            <kinematic_BC dof="3" node_ID="6" type="fixed"/>
        </field>
        <field field_name="electric_scalar_potential">
            <dof_labels>
                <String value="Psi"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="4" type="fixed"/>
            <kinematic_BC dof="1" node_ID="5" type="fixed"/>
            <kinematic_BC dof="1" node_ID="2" schedule="1" type="u" value="0.2"/>
        </field>
    </nodes>
    <element_list>
        <dielectric_elastomer_Q1P0
            electric_field_name="electric_scalar_potential"
            field_name="displacement" mass_type="lumped_mass">
            <hexahedron/>
            <solid_element_nodal_output displacements="1"
                electric_field="1" electric_scalar_potential="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_3D>
                    <Dielectric_Elastomer_Q1P0 Nrig="5.0" density="1.0"
                        epsilon="1.0" lambda="10000.0" mu="1.0"/>
                </large_strain_material_3D>
            </large_strain_element_block>
        </dielectric_elastomer_Q1P0>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-6"
        check_LHS_perturbation="1.0E-8" check_code="no_check"
        divergence_tolerance="1.0e+10" max_iterations="500" rel_tolerance="1.0e-8">
        <SPOOLES_matrix/>
    </nonlinear_solver>
</tahoe>

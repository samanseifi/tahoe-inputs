clear allclose allPsi_tot = [];Q = [];numfiles = 24;fileinitial = 'psi';for i = 0:numfiles-1    filename = [fileinitial num2str(i)];    textfilename = strcat(filename, '.csv');    data = csvread(textfilename, 1);    Psi = data(:,1);    Q =  data(:,2);    if i > 1        Psi = Psi + Psi_old;        Q = Q + Q_old;    end        plot(F_Psi, Psi)    hold onend    
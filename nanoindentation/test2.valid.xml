<?xml version='1.0' encoding='US-ASCII' standalone='no' ?>

<tahoe
              title='Dielectric Elastomer Testing'
             author='samansei'
    geometry_format='automatic'
      geometry_file='geometry/20x8.geom'
      output_format='ExodusII'
 restart_output_inc='0'
         echo_input='false'
            logging='moderate'
         compute_IC='true'>
	<time
	       scaling='linear'
	     init_time='0.000000e+00'
	     num_steps='100000'
	    output_inc='1'
	 max_step_cuts='0'
	     time_step='1.000000e-01'>
		<schedule_function>
			<piecewise_linear>
				<OrderedPair
				 x='0.000000e+00'
				 y='0.000000e+00'/>
				<OrderedPair
				 x='1.000000e+06'
				 y='1.000000e+06'/>
			</piecewise_linear>
		</schedule_function>
	</time>
	<nodes>
		<field
		              field_name='displacement'
		          solution_group='1'
		              integrator='static'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='D_X'/>
				<String
				 value='D_Y'/>
			</dof_labels>
			<kinematic_BC
			  node_ID='4'
			      dof='1'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
			<kinematic_BC
			  node_ID='1'
			      dof='2'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
			<sphere_augmented_Lagrangian
			            stiffness='1.000000e+00'
			 friction_coefficient='0.000000e+00'
			               radius='3.000000e+00'>
				<bc_initial_position>
					<Vector_2
					 v_1='-5.000000e+00'
					 v_2='5.000000e+00'/>
				</bc_initial_position>
				<bc_velocity>
					<Vector_2
					 v_1='0.000000e+00'
					 v_2='-5.000000e-01'/>
				</bc_velocity>
				<velocity_constant/>
				<node_ID_list>
					<String
					 value='3'/>
				</node_ID_list>
			</sphere_augmented_Lagrangian>
		</field>
		<field
		              field_name='electric_scalar_potential'
		          solution_group='1'
		              integrator='static'
		 total_energy_output_inc='0'>
			<dof_labels>
				<String
				 value='Psi'/>
			</dof_labels>
			<kinematic_BC
			  node_ID='1'
			      dof='1'
			     type='fixed'
			 schedule='0'
			    value='0.000000e+00'/>
			<kinematic_BC
			  node_ID='3'
			      dof='1'
			     type='u'
			 schedule='1'
			    value='0.000000e+00'/>
		</field>
		<history_node_ID_list>
			<String
			 value='2'/>
		</history_node_ID_list>
	</nodes>
	<element_list>
		<dielectric_elastomer_Q1P02D
		          field_name='displacement'
		           mass_type='automatic'
		      eigenvalue_inc='0'
		 electric_field_name='electric_scalar_potential'>
			<quadrilateral
			 num_ip='4'/>
			<solid_element_nodal_output
			             displacements='1'
			                    stress='1'
			     strain_energy_density='1'
			     electric_displacement='1'
			 electric_scalar_potential='1'
			                    strain='1'/>
			<large_strain_element_block>
				<block_ID_list>
					<String
					 value='1'/>
				</block_ID_list>
				<large_strain_material_2D>
					<Dielectric_Elastomer_Q1P02D
					       density='1.000000e+00'
					 constraint_2D='none'
					           CTE='0.000000e+00'
					            mu='1.000000e+00'
					       epsilon='1.000000e+00'
					          Nrig='5.000000e+00'
					        lambda='1.000000e+03'
					         gamma='0.000000e+00'
					           t_0='0.000000e+00'/>
				</large_strain_material_2D>
			</large_strain_element_block>
		</dielectric_elastomer_Q1P02D>
	</element_list>
	<nonlinear_solver
	               print_eqnos='false'
	                check_code='no_check'
	    check_LHS_perturbation='1.000000e-08'
	            max_iterations='700'
	            min_iterations='0'
	 reform_tangent_iterations='1'
	             abs_tolerance='1.000000e-07'
	             rel_tolerance='1.000000e-09'
	      divergence_tolerance='1.000000e+10'
	          quick_solve_iter='6'
	         quick_solve_count='3'
	                output_inc='0'>
		<profile_matrix/>
	</nonlinear_solver>
</tahoe>

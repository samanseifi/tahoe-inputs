<?xml version="1.0" encoding="UTF-8"?>
<tahoe author="samansei" compute_IC="false" echo_input="false"
    geometry_file="geometry/withdefect/defect5/defect5.geom" output_format="ExodusII"
    title="Dielectric Elastomer Testing" xmlns:x0="http://www.w3.org/2001/XMLSchema">
    <time num_steps="250000" output_inc="10" time_step="0.01">
        <schedule_function>
            <piecewise_linear>
                <OrderedPair x="0" y="0"/>
                <OrderedPair x="1000000" y="1000000"/>
            </piecewise_linear>
        </schedule_function>
    </time>
    <nodes>
        <field field_name="displacement" integrator="nonlinear_HHT">
            <dof_labels>
                <String value="D_X"/>
                <String value="D_Y"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="4" schedule="1" type="u" value="-0.025"/>
            <kinematic_BC dof="2" node_ID="5" type="fixed"/>
            <kinematic_BC dof="1" node_ID="3" schedule="1" type="u" value="0.025"/>
        </field>
        <field field_name="electric_scalar_potential" integrator="static">
            <dof_labels>
                <String value="Psi"/>
            </dof_labels>
            <kinematic_BC dof="1" node_ID="1" type="fixed"/>
            <kinematic_BC dof="1" node_ID="3" schedule="1" type="u" value="0.00"/>
        </field>
        <history_node_ID_list>
            <String value="2"/>
        </history_node_ID_list>
    </nodes>
    <element_list>
        <dielectric_elastomer_Q1P02D
            electric_field_name="electric_scalar_potential"
            field_name="displacement" mass_type="lumped_mass">
            <quadrilateral/>
            <solid_element_nodal_output displacements="1"
                electric_displacement="1" electric_scalar_potential="1"
                strain="1" strain_energy_density="1" stress="1"/>
            <large_strain_element_block>
                <block_ID_list>
                    <String value="1"/>
                </block_ID_list>
                <large_strain_material_2D>
                    <Dielectric_Elastomer_Q1P02D Nrig="1000000.0"
                        density="1.0" epsilon="1.0" gamma="0.0"
                        lambda="1000.0" mu="1.0" t_0="0.0"/>
                </large_strain_material_2D>
            </large_strain_element_block>
        </dielectric_elastomer_Q1P02D>
    </element_list>
    <nonlinear_solver abs_tolerance="1.0e-7"
        check_LHS_perturbation="1.0E-8" check_code="no_check"
        divergence_tolerance="1.0e+10" max_iterations="500" rel_tolerance="1.0e-9">
        <profile_matrix/>
    </nonlinear_solver>
</tahoe>
